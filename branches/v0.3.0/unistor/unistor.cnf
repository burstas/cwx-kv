#common conf
[common]
home=/usr/home/unistor/jpool/src/unistor/trunk/unistor/unistor
thread_num=4
store_type=bdb
sock_buf_kbyte=8192
max_chunk_kbyte=1024
store_flush_num=50000
host=172.121.222.122
idc=yf
group=1
monitor=*:9666
trans_conn_num=20
sync_conn_num=20
read_cache_mbyte=2048
read_cache_max_key_num=10000000
write_cache_mbyte = 128
master_lost_binlog = 100000
